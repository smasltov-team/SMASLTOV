//////////////////
// Core Predicates

foreach (z in AmbientNames) {
	%p z(v_1)
}

foreach (z in AmbientNames) {
	%p in[z](v_1)
	%p out[z](v_1)
	%p open[z](v_1)
}

foreach (l in Labels) {
	%p l(v_1)
}

%p bang(v)

%p pa(v_1, v_2) function

%p error()

/////////////////////////////
// Instrumentation Predicates 

foreach (z1 in AmbientNames) {
  foreach (z2 in AmbientNames) {
    %i inside[z1,z2]() = E(v_1, v_2) z1(v_1) & z2(v_2) & pa+(v_1, v_2) {}
  }
}