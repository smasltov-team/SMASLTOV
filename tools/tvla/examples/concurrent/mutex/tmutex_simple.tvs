#define UNBOUND_THREADS
#define TVM

#ifdef TVM
%t = { s_t(main) }
%n = { u }
%p = {
    #ifdef UNBOUND_THREADS
    sm = { s_t: 1/2 }
    #endif
    ready = { s_t }
    is_lock = { u } 
    global[aLock] = { u } 
}
#elseif
%n = { s_t, u }
%p = {
    #ifdef UNBOUND_THREADS
    sm = { s_t: 1/2 }
    #endif
    is_thread = { s_t }
    is_lock = { u } 
    at[l_1] = { s_t }
    runnable= { s_t : 1/2 }
    global[aLock] = { u } 
}
#endif
