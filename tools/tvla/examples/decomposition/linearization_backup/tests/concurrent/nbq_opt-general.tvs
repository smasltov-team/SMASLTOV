       #if(1 < GNUM)
       , tTwo(general_g2)
       #endif
       #if(2 < GNUM)
       , tThree(general_g3)
       #endif
       #if(3 < GNUM)
       , tFour(general_g4)
       #endif 
     }
%n = { uh }
%p = { 
    
    //is_node = {uh}
