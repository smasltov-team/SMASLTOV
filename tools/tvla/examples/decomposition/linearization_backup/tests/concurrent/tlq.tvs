//empty queue
%t = { tOne(general_one)
       #if(1 < TNUM)
       , tTwo(general_two)
       #endif
       #if(2 < TNUM)
       , tThree(general_three)
       #endif
       #if(3 < TNUM)
       , tFour(general_four)
       #endif 
     }
//%t = { tOne(general_one), tTwo(nop) }
%n = { uh }
%p = { 
    
    ready = { tOne 
              #if(1 < TNUM)
              , tTwo
              #endif
              #if(2 < TNUM)
              , tThree
              #endif
              #if(3 < TNUM)
              , tFour
              #endif 
            }
    
    r_by[Tail] = {uh}

    r_by[Tail_seq] = {uh}

    r_by[Head] = {uh}

    r_by[Head_seq] = {uh}

}



