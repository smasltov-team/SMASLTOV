/*****
* nbq.tvs
* Initial structure for nonblocking queue implementation.
* head always points to dummy node. 
*****/

//All local variables initially null

//empty queue
//%t = { tOne(general_one), tTwo(nop) }
%t = { tOne(general_g1)
       #if(1 < GNUM)
       , tTwo(general_g2)
       #endif
       #if(2 < GNUM)
       , tThree(general_g3)
       #endif
       #if(3 < GNUM)
       , tFour(general_g4)
       #endif 
     }
%n = { uh }
%p = { 
    
    //is_node = {uh}
    ready = { tOne 
              #if(1 < GNUM)
              , tTwo
              #endif
              #if(2 < GNUM)
              , tThree
              #endif
              #if(3 < GNUM)
              , tFour
              #endif 
            }
    
    //is_name[one] = {tprd}
    //is_name[two] = {tcns}
    
    r_by[Tail] = {uh}

    r_by[Tail_seq] = {uh}

    r_by[Head] = {uh}

    r_by[Head_seq] = {uh}

}


