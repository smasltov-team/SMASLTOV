%n = {readers, writers, n0, n1, s_succ}
%p = {
    isThread = {readers, writers}
    sm = {s_succ:1/2, readers:1/2, writers:1/2}
    reader = {readers}
    writer = {writers}
    at[rl_1] = {readers}
    at[wl_1] = {writers}

    mutex = 1
    w = 1

    num0 = {n0}
    num1 = {n1}
    successor = {n0->n1, n1->s_succ:1/2, s_succ->s_succ:1/2}
    rt[successor] = {n0, n1, s_succ}

    readcount = {n0}
    readersInCrit = {n0}
}
