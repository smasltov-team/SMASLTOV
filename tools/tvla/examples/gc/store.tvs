%p = {
  sm = {u0:1/2, u1:1/2}
  
	root = {u0:1/2}
  
	left = {u0 -> u0:1/2, u1 -> u1:1/2, u1 -> u0:1/2}
  
	right = {u0 -> u0:1/2, u1 -> u1:1/2, u1 -> u0:1/2}
  
	r[root] = {u0}
  
	unique_selection = {u0:1/2, u1:1/2}

